using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace AWGP
{
    public class EventArgs<T> : EventArgs
    {
        public EventArgs(T value)
        {
            this.value = value;
        }

        public T Value
        {
            get { return value; }
        }

        private T value;
    }
}
